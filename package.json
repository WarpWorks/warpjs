  },
  "peerDependencies": {
    "@quoin/expressjs-routes-info": ">=2.0.1",
    "@warp-works/warpjs-plugins": ">=2.0.1",
    "@warp-works/warpjs-session-plugin": ">=2.0.2",
    "@warp-works/warpjs-utils": ">=2.0.3",
    "body-parser": ">=1.17.0",
    "cookie-parser": ">=1.4.0",
    "express": ">=4.0.0"
  },
  "mocha": {
    "diff": true,
    "exit": true,
    "recursive": true,
    "spec": [
      "lib/**/*.test.js",
      "client/**/*.test.js",
      "server/**/*.test.js"
    ]
  },
  "nyc": {
    "all": true,
    "temp-dir": "./reports/nyc_output",
    "report-dir": "./reports/coverage",
    "check-coverage": false,
    "include": [
      "lib/**/*.js",
      "client/**/*.js",
      "client/**/*.jsx",
      "server/**/*.js"
    ],
    "exclude": [
      "**/*.*.test.js",
      "Gruntfile.js",
      "grunt/**/*.js",
      "public/3rd-party",
      "public/app",
      "public/libs",
      "reports",
      "server.js",
      "studio"
    ],
    "reporter": [
      "cobertura",
      "lcov",
      "html"
    ],
    "watermarks": {
      "statements": [
        50,
        80
      ],
      "lines": [
        50,
        80
      ],
      "functions": [
        50,
        80
      ],
      "branches": [
        50,
        80
      ]
    }
  }
}
