    "grunt-contrib-less": "^3.0.0",
    "grunt-eslint": "^23.0.0",
    "grunt-webpack": "3.1.x",
    "handlebars": "^4.7.7",
    "handlebars-loader": "1.7.x",
    "jscpd": "^3.3.26",
    "load-grunt-config": "~3.0.1",
